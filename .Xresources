URxvt*scrollBar: false
URxvt*transparent: false
URxvt*depth: 32

URxvt*font: xft:xos4 Terminess Powerline:style=Regular:pixelsize=14
URxvt.boldFont: *-terminus-bold-*-normal-*-14-*-*-*-*-*-*
URxvt*letterSpace: 0
URxvt*lineSpace: 2
URxvt*cursorUnderline: true

URxvt*internalBorder: 20 

! special
*.foreground:   #ffffff
*.background:   #161616
*.cursorColor:  #ffffff

! black
*.color0:       #222222
*.color8:       #666666

! red
*.color1:       #e84f4f
*.color9:       #d23d3d

! green
*.color2:       #b7ce42
*.color10:      #bde077

! yellow
*.color3:       #fea63c
*.color11:      #ffe863

! blue
*.color4:       #66aabb
*.color12:      #aaccbb

! magenta
*.color5:       #b7416e
*.color13:      #e16a98

! cyan
*.color6:       #6d878d
*.color14:      #42717b

! white
*.color7:       #dddddd
*.color15:      #cccccc


rofi.font: xos4\ Terminus 14
rofi.color-window: argb:80191919, argb:60181818, #191919
rofi.color-normal: argb:0095C7AE, #A09894, argb:0095C7AE, argb:80101218, #95C7A0
rofi.padding: 400
rofi.separator-style: none
